<?xml version="1.0" encoding="UTF-8"?>
<cibet xmlns="http://www.logitags.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.logitags.com 
   ../../main/resources/cibet-config_1.0.xsd">

	<setpoint id="A">
		<controls>
			<tenant>ten1</tenant>
			<event>INSERT, INVOKE</event>
			<target>xxx*</target>
		</controls>
		<actuator name="ARCHIVE"/>
	</setpoint>

	<setpoint id="B1">
		<controls>
			<tenant>ten1|x</tenant>
			<event>UPDATE</event>
			<target>*</target>
		</controls>
		<actuator name="ARCHIVE"/>
	</setpoint>

	<setpoint id="B2">
		<controls>
			<tenant>ten1|x</tenant>
			<event>UPDATE, INSERT</event>
			<target>com.nix,com.nix.*, com.cibethelper.entities.*,com.cibethelper.base.*</target>
		</controls>
		<actuator name="ARCHIVE"/>
	</setpoint>

	<setpoint id="B3">
		<controls>
			<tenant>ten1|x</tenant>
			<event>UPDATE, INSERT</event>
			<target>com.cibethelper.*</target>
		</controls>
		<actuator name="ARCHIVE"/>
	</setpoint>

	<setpoint id="C">
		<controls>
			<event>INSERT</event>
			<target>com.logitags.cibet.Nix, com.cibethelper.entities.TEntity</target>
			<stateChange exclude="true">counter</stateChange>
		</controls>
		<actuator name="ARCHIVE"/>
	</setpoint>

	<setpoint id="D">
		<controls>
			<tenant>y</tenant>
			<event>INSERT</event>
		</controls>
		<actuator name="ARCHIVE"/>
	</setpoint>

	<setpoint id="XX">
		<controls>
			<tenant>other</tenant>
			<target>"Hastela, VV " , com.logitags.cibet.Nix, com.cibethelper.entities.TE;" \"Hase"</target>
		</controls>
		<actuator name="ARCHIVE"/>
	</setpoint>

</cibet>   