<?xml version="1.0" encoding="UTF-8"?>
<cibet xmlns="http://www.logitags.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.logitags.com ../../main/resources/cibet-config_1.4.xsd"
	name="custControl">


	<control name="SEC1">
		<class>com.cibethelper.base.FalseCustomControl</class>
		<properties>
			<ga>45</ga>
		</properties>
	</control>

	<control name="TRUE">
		<class>com.cibethelper.base.TrueCustomControl</class>
		<properties>
			<gaga>45</gaga>
		</properties>
	</control>

	<actuator name="FOUR_EYES">
		<properties>
			<throwPostponedException>true</throwPostponedException>
		</properties>
	</actuator>

	<setpoint id="K1">
		<controls>
			<event>
				<include>INVOKE</include>
			</event>
			<customControls>
				<customControl name="TRUE">
					<include>dsfg</include>
				</customControl>
			</customControls>
			<target>
				<include>com.cibethelper.entities.TComplexEntity</include>
				<include>http://localhost:9090/webapp/other*</include>
				<include>http://localhost:9190/webapp/other*</include>
			</target>
			<method>
				<include>setOwner</include>
				<include>GET</include>
				<include>POST</include>
			</method>
			<invoker>
				<include>fff</include>
			</invoker>
		</controls>
		<actuator name="INFOLOG"/>
	</setpoint>

	<setpoint id="K2">
		<controls>
			<event>
				<include>INVOKE</include>
			</event>
			<customControls>
				<customControl name="TRUE"></customControl>
			</customControls>
			<target>
				<include>com.cibethelper.entities.TComplexEntity</include>
				<include>http://localhost:9090/webapp/other*</include>
				<include>http://localhost:9190/webapp/other*</include>
			</target>
			<method>
				<include>setOwner</include>
				<include>GET</include>
				<include>POST</include>
			</method>
			<invoker></invoker>
		</controls>
		<actuator name="INFOLOG"/>
	</setpoint>

	<setpoint id="K3">
		<controls>
			<event>
				<include>INVOKE</include>
			</event>
			<customControls>
				<customControl name="TRUE" />
			</customControls>
			<target>
				<include>com.cibethelper.entities.TComplexEntity</include>
				<include>http://localhost:9090/webapp/other*</include>
				<include>http://localhost:9190/webapp/other*</include>
			</target>
			<method>
				<include>setOwner</include>
				<include>GET</include>
				<include>POST</include>
			</method>
			<invoker />
		</controls>
		<actuator name="INFOLOG"/>
	</setpoint>

</cibet>   