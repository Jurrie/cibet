<?xml version="1.0" encoding="UTF-8"?>
<cibet xmlns="http://www.logitags.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.logitags.com 
   ../../main/resources/cibet-config_1.0.xsd">

	<actuator name="SEC1">
		<class>com.logitags.cibet.actuator.shiro.ShiroActuator</class>
		<properties>
			<requiresAuthentication></requiresAuthentication>
		</properties>
	</actuator>

	<actuator name="SEC2">
		<class>com.logitags.cibet.actuator.shiro.ShiroActuator</class>
		<properties>
			<requiresGuest>true</requiresGuest>
		</properties>
	</actuator>

	<actuator name="SEC3">
		<class>com.logitags.cibet.actuator.shiro.ShiroActuator</class>
		<properties>
			<hasAllRoles>goodguy, nottt</hasAllRoles>
		</properties>
	</actuator>

	<actuator name="SEC4">
		<class>com.logitags.cibet.actuator.shiro.ShiroActuator</class>
		<properties>
			<hasAllRoles>goodguy, schwartz </hasAllRoles>
		</properties>
	</actuator>

	<actuator name="SEC5">
		<class>com.logitags.cibet.actuator.shiro.ShiroActuator</class>
		<properties>
			<isPermittedAll>winnebago:drive:eagle5 </isPermittedAll>
		</properties>
	</actuator>

	<actuator name="SEC6">
		<class>com.logitags.cibet.actuator.shiro.ShiroActuator</class>
		<properties>
			<isPermittedAll>ccc:sdsd:eagle5 </isPermittedAll>
		</properties>
	</actuator>

	<actuator name="SEC7">
		<class>com.logitags.cibet.actuator.shiro.ShiroActuator</class>
		<properties>
			<requiresUser/>
		</properties>
	</actuator>

	<actuator name="SEC8">
		<class>com.logitags.cibet.actuator.shiro.ShiroActuator</class>
		<properties>
			<secondPrincipal>true</secondPrincipal>
			<throwDeniedException>true</throwDeniedException>
			<requiresUser>true</requiresUser>
			<isPermittedAll>ccc:sdsd:eagle5 </isPermittedAll>
		</properties>
	</actuator>

	<actuator name="SEC9">
		<class>com.logitags.cibet.actuator.shiro.ShiroActuator</class>
		<properties>
			<secondPrincipal/>
			<throwDeniedException></throwDeniedException>
			<requiresUser/>
			<isPermittedAll>jaeger:schiess:Reh </isPermittedAll>
			<!--hasAllRoles>badguy</hasAllRoles-->
		</properties>
	</actuator>

	<setpoint id="SHIRO1">
		<controls>
			<event>INVOKE</event>
			<target>http://localhost:8788/HttpShiroIT/ts/shiro1,http://localhost:9190/webapp/ts/shiro1</target>
			<method>GET,POST</method>
		</controls>
		<actuator name="SEC1"/>
	</setpoint>

	<setpoint id="SHIRO1DENY">
		<controls>
			<event>INVOKE</event>
			<target>http://localhost:8788/HttpShiroIT/ts/shiro2,http://localhost:9190/webapp/ts/shiro2</target>
			<method>GET,POST</method>
		</controls>
		<actuator name="SEC2"/>
	</setpoint>

	<setpoint id="SHIRO1DENY2">
		<controls>
			<event>INVOKE</event>
			<target>http://localhost:8788/HttpShiroIT/ts/shiro3,http://localhost:9190/webapp/ts/shiro3</target>
			<method>GET,POST</method>
		</controls>
		<actuator name="SEC3"/>
	</setpoint>

	<setpoint id="SHIRO3">
		<controls>
			<event>INVOKE</event>
			<target>http://localhost:8788/HttpShiroIT/ts/shiro4,http://localhost:9190/webapp/ts/shiro4</target>
			<method>GET,POST</method>
		</controls>
		<actuator name="SEC4"/>
	</setpoint>

	<setpoint id="SHIRO4">
		<controls>
			<event>INVOKE</event>
			<target>http://localhost:8788/HttpShiroIT/ts/shiro5,http://localhost:9190/webapp/ts/shiro5</target>
			<method>GET,POST</method>
		</controls>
		<actuator name="SEC5"/>
	</setpoint>

	<setpoint id="SHIRO5">
		<controls>
			<event>INVOKE</event>
			<target>http://localhost:8788/HttpShiroIT/ts/shiro6,http://localhost:9190/webapp/ts/shiro6</target>
			<method>GET,POST</method>
		</controls>
		<actuator name="SEC6"/>
	</setpoint>

	<setpoint id="SHIRO6">
		<controls>
			<event>INVOKE</event>
			<target>http://localhost:8788/HttpShiroIT/ts/shiro7,http://localhost:9190/webapp/ts/shiro7</target>
			<method>GET,POST</method>
		</controls>
		<actuator name="SEC7"/>
	</setpoint>

	<setpoint id="SHIRO7">
		<controls>
			<event>INVOKE</event>
			<target>http://localhost:8788/HttpShiroIT/ts/shiro8,http://localhost:9190/webapp/ts/shiro8</target>
			<method>GET,POST</method>
		</controls>
		<actuator name="SEC8"/>
		<actuator name="SEC5"/>
		<actuator name="SEC7"/>
	</setpoint>

	<setpoint id="SHIRO8">
		<controls>
			<event>INVOKE</event>
			<target>http://localhost:8788/HttpShiroIT/ts/shiro9,http://localhost:9190/webapp/ts/shiro9</target>
			<method>GET,POST</method>
		</controls>
		<actuator name="SEC5"/>
		<actuator name="SEC7"/>
		<actuator name="SEC9"/>
	</setpoint>

	<setpoint id="SHIRO2">
		<controls>
			<event>INVOKE</event>
			<target>http://localhost:8080/ejbwar/ts, http://localhost:8788/HttpShiroIT/eee, http://localhost:9190/webapp/eee</target>
			<method>OPTIONS</method>
		</controls>
		<actuator name="SEC2"/>
	</setpoint>

</cibet>   