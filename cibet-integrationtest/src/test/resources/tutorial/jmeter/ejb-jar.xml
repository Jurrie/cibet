<?xml version="1.0" encoding="UTF-8"?>  
<ejb-jar version="3.0"   
     xmlns="http://java.sun.com/xml/ns/javaee"  
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"   
     xsi:schemaLocation="http://java.sun.com/xml/ns/javaee   
     http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd">  

  <assembly-descriptor>
    <interceptor-binding>
      <ejb-name>MonitorEjb</ejb-name>
      <interceptor-class>com.logitags.cibet.sensor.ejb.CibetInterceptor</interceptor-class>
      <method>
         <method-name>cibetCalc</method-name>
      </method>
    </interceptor-binding>
    <interceptor-binding>
      <ejb-name>MonitorEjb</ejb-name>
      <interceptor-class>com.logitags.cibet.sensor.ejb.CibetInterceptor</interceptor-class>
      <method>
         <method-name>cibetIo</method-name>
      </method>
    </interceptor-binding>
  </assembly-descriptor>
</ejb-jar>
